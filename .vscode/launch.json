{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Program",
			"type": "node",
			"request": "launch",
			"args": ["src/index.ts"],
			"runtimeArgs": ["-r", "ts-node/register"],
			"cwd": "${workspaceRoot}",
			"internalConsoleOptions": "openOnSessionStart"
		},
		{
			"name": "CLI - generate --config-json",
			"type": "node",
			"request": "launch",
			"args": ["src/cli/index.ts", "generate", "--config", "./build/config.json"],
			"runtimeArgs": ["-r", "ts-node/register"],
			"cwd": "${workspaceRoot}",
			"internalConsoleOptions": "openOnSessionStart",
			"console": "internalConsole",
			"outputCapture": "std"
		},
		{
			"name": "CLI - generate",
			"type": "node",
			"request": "launch",
			"args": [
				"src/cli/index.ts",
				"generate",
				"-g",
				"ng-typescript",
				"-i",
				"../swagger-api.json",
				"-o",
				"./build/codegen",
				"--clean"
			],
			"runtimeArgs": ["-r", "ts-node/register"],
			"cwd": "${workspaceRoot}",
			"internalConsoleOptions": "openOnSessionStart",
			"console": "internalConsole",
			"outputCapture": "std"
		},
		{
			"name": "CLI - generate -h",
			"type": "node",
			"request": "launch",
			"args": ["src/cli/index.ts", "generate", "-h"],
			"runtimeArgs": ["-r", "ts-node/register"],
			"cwd": "${workspaceRoot}",
			"internalConsoleOptions": "openOnSessionStart",
			"console": "internalConsole",
			"outputCapture": "std"
		},
		{
			"name": "CLI - help",
			"type": "node",
			"request": "launch",
			"args": ["src/cli/index.ts", "--help"],
			"runtimeArgs": ["-r", "ts-node/register"],
			"cwd": "${workspaceRoot}",
			"internalConsoleOptions": "openOnSessionStart",
			"console": "internalConsole",
			"outputCapture": "std"
		}
	]
}
