{
	"name": "kodgen",
	"version": "0.0.1",
	"description": "",
	"main": "dist/src/cli/cli.js",
	"bin": {
		"kodgen": "dist/src/cli/cli.js"
	},
	"types": "dist/src/index.d.ts",
	"scripts": {
		"build": "rimraf dist && mkdir dist && tsc --project tsconfig.build.json",
		"postbuild": "copyfiles src/**/*.ejs dist",
		"test": "jest",
		"test:watch": "jest --watch",
		"lint": "eslint src/**/*.ts",
		"format:check": "prettier --list-different \"**/*{.ts,.js,.json}\"",
		"format:write": "prettier -w --list-different \"**/*{.ts,.js,.json}\"",
		"prepack": "npm run build",
		"prepublishOnly": "npm run test",
		"preversion": "npm run lint && npm run format:check && npm run test"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/MacRdy/kodgen.git"
	},
	"author": "Anton Shibaev",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/MacRdy/kodgen/issues"
	},
	"homepage": "https://github.com/MacRdy/kodgen#readme",
	"files": [
		"dist/**/*"
	],
	"devDependencies": {
		"@types/ejs": "^3.1.1",
		"@types/jest": "^29.2.0",
		"@types/yargs": "^17.0.13",
		"@typescript-eslint/eslint-plugin": "^5.41.0",
		"@typescript-eslint/parser": "^5.41.0",
		"copyfiles": "^2.4.1",
		"eslint": "^8.26.0",
		"eslint-plugin-optimize-regex": "^1.2.1",
		"eslint-plugin-sonarjs": "^0.16.0",
		"jest": "^29.2.1",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2",
		"ts-jest": "^29.0.3",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.4"
	},
	"dependencies": {
		"@apidevtools/swagger-parser": "^10.1.0",
		"ajv": "^8.11.2",
		"camel-case": "^4.1.2",
		"ejs": "^3.1.8",
		"just-kebab-case": "^4.1.1",
		"openapi-types": "^12.0.2",
		"pascal-case": "^3.1.2",
		"yargs": "^17.6.0"
	}
}
